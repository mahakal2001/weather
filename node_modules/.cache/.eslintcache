    setDescription(data.weather[0].description)\n    setIcon(data.weather[0].icon)\n    setHumidity(data.main.humidity)\n    setWind(data.wind.speed)\n    setCountry(data.sys.country)\n  \n    setDataFetched(true)\n  }catch(err){\n    console.log(err)\n    alert(\"Please enter a valid location\")\n  }\n\n}\n\nconst defaultDataFetched = async () =>{\n  if(!dataFetched){\n    const res = await axios.get(`https://api.openweathermap.org/data/2.5/weather?q=tamluk&appid=ff0d20bdc9534107da28acc490028535&units=metric`)\n    const data = await res.data\n  \n    setDegrees(data.main.temp)\n    setLocation(data.name)\n    setDescription(data.weather[0].description)\n    setIcon(data.weather[0].icon)\n    setHumidity(data.main.humidity)\n    setWind(data.wind.speed)\n    setCountry(data.sys.country)\n  }\n\n}\n\nuseEffect(() => {\n  defaultDataFetched()\n}, [])\n\n  return (\n    <div className=\"App\">\n        <div className='weather'>\n            <Input \n              text={(e) => setuserLocation(e.target.value)}\n              submit={fetchData}\n              func={fetchData}\n            />\n\n            <div className='weather_display'>\n            <h3 className='weather_location'>{location}, {country}</h3>\n\n            <div>\n              <h1 className='weather_degrees'>{degrees} Â°C</h1>\n            </div>\n\n            <div className='weather_description'>\n              <div >\n                <div className='weather_description_head'>\n                    <span className='weather_icon'>\n                      <img src={`http://openweathermap.org/img/w/${icon}.png`} alt=\"weather icon\" />\n                    </span>\n                    <h3>{description}</h3>\n                </div>\n\n                <h3>Humidity: {humidity}%</h3>\n                <h3>Wind speed: {wind} m/s</h3>\n              </div>\n\n              \n            </div>\n\n        </div>\n\n        </div>\n    </div>\n  );\n}\n\nexport default Weather;","/home/mahakal2001/Documents/basic